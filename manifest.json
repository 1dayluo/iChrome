{
	"content_scripts": [
		{
			"js": [ "oauth2/oauth2_inject.js" ],
			"matches": [ "https://ichrome.uservoice.com/robots.txt*", "https://www.google.com/ichrome/robots.txt*", "https://developers.google.com/robots.txt*" ],
			"run_at": "document_start"
		},
		{
			"js": [ "js/wunderlistfix.js" ],
			"matches": [ "https://*.wunderlist.com/webapp*", "http://*.wunderlist.com/webapp*" ],
			"run_at": "document_start",
			"all_frames": true
		},
		{
			"css": [ "css/gmailfix.css" ],
			"matches": [ "https://mail.google.com/*" ],
			"all_frames": true
		},
		{
			"css": [ "css/gplus.css" ],
			"matches": [ "https://plus.google.com/app/basic*" ],
			"all_frames": true
		},
		{
			"js": [ "js/facebook.js" ],
			"matches": [ "https://m.facebook.com/*" ],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
	"web_accessible_resources": [
		"oauth2/oauth2.html",
		"index.html"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	},
	"permissions": [
		"storage",
		"identity",
		"topSites",
		"http://*/",
		"https://*/",
		"management",
		"webRequest",
		"unlimitedStorage",
		"chrome://favicon/",
		"webRequestBlocking"
	],
	"oauth2": {
		"client_id": "1092601450132-rsgske9erdebomsgtbp0sa2sj0qh3md3.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/googlenow"]
	},
	"version": "2.1.0",
	"manifest_version": 2,
	"short_name": "iChrome New Tab",
	"offline_enabled": true,
	"optional_permissions": ["tabs"],
	"minimum_chrome_version": "29.0",
	"name": "iChrome New Tab",
	"update_url": "http://clients2.google.com/service/update2/crx",
	"key": "",
	"description": "Your homepage, for Chrome. Now for new tab.",
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://*.uservoice.com https://www.google.com; object-src 'self'"
}

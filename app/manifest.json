{
	"content_scripts": [
		{
			"js": [ "oauth2/oauth2_inject.js" ],
			"matches": [ "https://ichrome.uservoice.com/robots.txt*", "https://www.google.com/ichrome/robots.txt*", "https://developers.google.com/robots.txt*" ],
			"run_at": "document_start"
		},
		{
			"js": [ "inject/js/wunderlistfix.js" ],
			"matches": [ "https://*.wunderlist.com/*", "http://*.wunderlist.com/*" ],
			"run_at": "document_start",
			"all_frames": true
		},
		{
			"css": [ "inject/css/gmailfix.css" ],
			"matches": [ "https://mail.google.com/*" ],
			"all_frames": true
		},
		{
			"css": [ "inject/css/gplus.css" ],
			"matches": [ "https://plus.google.com/app/basic*" ],
			"all_frames": true
		},
		{
			"js": [ "inject/js/facebook.js" ],
			"matches": [ "https://m.facebook.com/*" ],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"chrome_url_overrides" : {
		"newtab": "index.html"
	},
	"web_accessible_resources": [
		"oauth2/oauth2.html",
		"index.html"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	},
	"permissions": [
		"storage",
		"topSites",
		"http://*/",
		"https://*/",
		"management",
		"webRequest",
		"unlimitedStorage",
		"chrome://favicon/",
		"webRequestBlocking"
	],
	"version": "2.1.9",
	"manifest_version": 2,
	"default_locale": "en",
	"short_name": "iChrome",
	"offline_enabled": true,
	"name": "__MSG_extName__",
	"optional_permissions": ["tabs"],
	"minimum_chrome_version": "29.0",
	"description": "__MSG_extDescription__",
	"update_url": "http://clients2.google.com/service/update2/crx",
	"key": "",
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://*.uservoice.com https://www.google.com; object-src 'self'"
}
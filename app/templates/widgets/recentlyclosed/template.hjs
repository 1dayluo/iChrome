{{#title}}<h2 class="header">{{title}}</h2>{{/title}}

<div class="items">
	{{#tabs}}
		<a class="item" href="{{url}}{{#newTab}}" target="_blank{{/newTab}}">
			<img class="favicon" src="chrome://favicon/{{url}}" />
			<span class="title">{{title}}</span>
		</a>
	{{/tabs}}
	{{^tabs}}
		<div class="item no-items">{{i18n.notabs}}</div>
	{{/tabs}}
</div>